{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy:dev": "firebase -P dev --token  $FIREBASE_TOKEN --non-interactive deploy --only hosting",
    "deploy:beta": "firebase -P beta --token  $FIREBASE_TOKEN --non-interactive deploy --only hosting",
    "deploy:prod": "firebase -P prod --token  $FIREBASE_TOKEN --non-interactive deploy --only hosting",
    "build:local": "env-cmd -f ./.env.production.local npm run build",
    "build:production": "env-cmd -f ./.env.production npm run build"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-traffic": "^1.0.2",
    "@react-navigation/bottom-tabs": "^5.11.10",
    "@react-navigation/native": "^5.9.4",
    "airtable": "^0.10.1",
    "algoliasearch": "^4.8.3",
    "bootstrap": "^4.6.0",
    "contentful": "^8.2.0",
    "firebase": "^8.0.1",
    "graphql": "^15.5.0",
    "hamburger-react": "^2.4.0",
    "lodash": "4.17.21",
    "mapbox-gl": "^2.0.0",
    "moment": "^2.29.1",
    "react": "^16.12.0",
    "react-compound-slider": "^3.3.1",
    "react-contentful": "^2.0.31",
    "react-dom": "^16.12.0",
    "react-instantsearch-dom": "^6.8.2",
    "react-mapbox-gl": "^5.1.1",
    "react-moment": "^0.9.7",
    "react-player": "^2.9.0",
    "react-qr-svg": "^2.3.0",
    "react-responsive": "^8.2.0",
    "react-responsive-carousel": "^3.2.18",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-use": "^17.2.4",
    "reactstrap": "^8.9.0",
    "rxfire": "3.13.5",
    "rxjs": "6.6.3",
    "styled-components": "^5.2.1",
    "web-vitals": "^1.0.1",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "prettier": "2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
